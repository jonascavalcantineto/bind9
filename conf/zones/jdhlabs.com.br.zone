$TTL 3600	; 1 hour
@	IN SOA	ocp.jdhlabs.com.br. netmaster.ocp.jdhlabs.com.br. (
				2020030501 ; serial
				86400      ; refresh (1 day)
				3600       ; retry (1 hour)
				86400      ; expire (1 day)
				3600       ; minimum (1 hour)
				)
@			IN 	NS	ns1

;Proxy LoadBalancers
ns1			IN A		10.11.2.3
bastion		IN A		10.11.2.3
bootstrap	IN A		10.11.2.4
nfs			CNAME		bastion
lb			CNAME		bastion
;lb			IN A		10.11.2.3

;Control Plannel (Masters Servers)
master1			IN A		10.11.2.2
ose				IN A		10.11.2.2
ose-internal	IN A		10.11.2.2
etcd-0			IN A		10.11.2.2

;Workers
worker1		IN A		10.11.2.5
infra1		CNAME		worker1
worker2		IN A		10.11.2.6
infra2		CNAME		worker2
worker3		IN A		10.11.2.7
apps1		CNAME		worker3
worker4		IN A		10.11.2.8
apps2		CNAME		worker4

;OCP|Kubernestes API  and APPS whithout LoadBalancer
api			A		10.11.2.4
;			A  		10.11.2.2

api-int		A		10.11.2.4
;			A  		10.11.2.2

*.apps		A			10.11.2.5
			A			10.11.2.6

openshift-bastion CNAME bastion
;OCP|Kubernestes API  and APPS
;api			CNAME		lb
;api-int		CNAME		lb
;*.apps		CNAME		lb

; ETCDs services 
_etcd-server-ssl._tcp	IN SRV	0	10	2380	etcd-0
